{
  "name": "geo",
  "version": "1.0.0",
  "description": "A single-container Node.js service that analyzes brands by:",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "nest build",
    "build:frontend": "webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "start": "nest start",
    "start:dev": "concurrently \"nest start --watch\" \"webpack --watch --config webpack.config.js\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:brand-insight": "./run-test.sh",
    "setup": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coach86/geo.git"
  },
